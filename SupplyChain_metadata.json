		"userdoc": {
			"kind": "user",
			"methods": {
				"acceptTransfer(string)": {
					"notice": "Accept transfer: recipient calls this to accept. This two-step pattern reduces unauthorized updates (simulates consensus between two parties)."
				},
				"addNote(string,string)": {
					"notice": "Helper to add a freeform note to history by owner (audit log)"
				},
				"assignRole(address,uint8)": {
					"notice": "ADMIN functions to assign roles"
				},
				"createProduct(string,string)": {
					"notice": "Manufacturer creates a product record. metadata can be an IPFS URI or encrypted payload (off-chain encryption recommended for private data)"
				},
				"getProductHistory(string)": {
					"notice": "Read full history â€” only permitted addresses can view. This simulates private channels / private data."
				},
				"getProductSummary(string)": {
					"notice": "Read product summary - limited information; full history only if viewPermission granted."
				},
				"grantView(string,address)": {
					"notice": "Grant and revoke view permission (admin or owner can grant)"
				},
				"initiateTransfer(string,address)": {
					"notice": "Initiate transfer: only current owner may initiate. This sets a pendingRecipient that must accept."
				},
				"markReceived(string)": {
					"notice": "Mark received by final recipient (could be same as acceptTransfer or separate)"
				}
			},
			"notice": "Manufacturer -> Distributor -> Retailer lifecycle with role-based access, events, history, private-view simulation, and two-phase transfer to simulate consensus.",
			"version": 1
		}
